<!--components/list/list.wxml-->
<view class="o-list" bindtap="toRead">
  <view>
    <o-img class="cover" src="https://statics.zhuishushenqi.com{{list.cover}}"></o-img>
  </view>
  <view class="o-intro">
    <p class="title">{{list.title}}</p>
    <p class="author">{{list.author}}</p>
    <p class="lastChapter">{{list.lastChapter}}</p>
  </view>
</view>
