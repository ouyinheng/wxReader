.page-group {
  width: 100%;
  height: 100vh;
}
.w-100 {
    width: 100%;
}
.header {
  width: 100%;
  padding: 10px;
  background-color: #EBEEF5;
  display: flex;
  box-sizing: border-box;
}
.input {
  width: 100%;
  height: 30px;
  line-height: 30px;
  background-color: white;
  padding: 5px 10px;
  font-size: 14px;
  color: black;
}
.history, .section {
  width: 100%;
  box-sizing: border-box;
  padding: 10px;
}
.title {
  font-size: 14px;
  color: #909399;
}
.his-item {
  font-size: 14px;
  padding: 5px 0;
}
.flex-center {
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.flex-center text {
  font-size: 16px;
}
.cu-card>.cu-item {
  margin-top: 10px!important;
}